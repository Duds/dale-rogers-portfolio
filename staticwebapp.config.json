{
  "routes": [
    {
      "route": "/_astro/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/favicon.svg",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/work/*",
      "rewrite": "/work/index.html"
    },
    {
      "route": "/articles/*",
      "rewrite": "/articles/index.html"
    },
    {
      "route": "/services/*",
      "rewrite": "/services/index.html"
    },
    {
      "route": "/tags/*",
      "rewrite": "/tags/index.html"
    },
    {
      "route": "/about",
      "rewrite": "/about/index.html"
    },
    {
      "route": "/contact",
      "rewrite": "/contact/index.html"
    },
    {
      "route": "/capabilities",
      "rewrite": "/capabilities/index.html"
    },
    {
      "route": "/colophon",
      "rewrite": "/colophon/index.html"
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/_astro/*", "/*.{css,js,png,jpg,jpeg,gif,svg,ico,woff,woff2}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}
