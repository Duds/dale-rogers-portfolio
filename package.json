{
  "name": "dale-rogers-portfolio",
  "type": "module",
  "version": "1.0.6",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "astro dev",
    "dev:turbo": "astro dev --turbo",
    "dev:health": "pnpm run check-pm && pnpm run check-tokens && pnpm run typecheck && pnpm run lint",
    "build": "astro build",
    "build:swa": "astro build && echo 'Build complete for Azure Static Web Apps'",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest --run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "gen:tree": "node scripts/gen-tree.cjs",
    "rules:seal": "node scripts/ensure-rules-frontmatter.cjs",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:export": "storybook build --docs",
    "check-pm": "node scripts/check-package-manager.js",
    "check-tokens": "node scripts/check-tokens.cjs",
    "migrate:theme": "node scripts/migrate-theme-colors.js",
    "fix:css": "node scripts/fix-css-classes.js",
    "fix:all": "node scripts/fix-all-classes.js",
    "audit:images": "node src/scripts/audit-images.js",
    "audit:images:fix": "node src/scripts/audit-images.js --fix",
    "prepare": "husky",
    "commit": "git-cz",
    "commit:lint": "commitlint --edit",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "version:sync": "node scripts/sync-version.js"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,astro,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.2.6",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/tailwind": "^6.0.2",
    "@iconify-icons/carbon": "^1.2.20",
    "@iconify-icons/mdi": "^1.2.48",
    "@iconify-icons/simple-icons": "^1.2.74",
    "@iconify/react": "^5.2.1",
    "@types/lodash-es": "^4.17.12",
    "dotenv": "^16.5.0",
    "lodash-es": "^4.17.21",
    "lucide-astro": "^0.540.0",
    "lucide-react": "^0.540.0",
    "node-fetch": "^3.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.52.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-docgen-typescript": "2.2.4-next.0",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/typography": "0.5.9",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "astro": "^5.7.13",
    "astro-eslint-parser": "^1.2.2",
    "clsx": "^2.1.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "glob": "^8.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "playwright": "^1.52.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "semantic-release": "^24.2.7",
    "sharp": "^0.34.3",
    "storybook": "^8.6.14",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.12",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-bundle-analyzer": "^1.2.1",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
